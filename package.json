{
  "name": "koliseu-launcher",
  "version": "1.0.0",
  "description": "KoliseuOT Game Launcher",
  "author": "KoliseuOT Team",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "tsc && vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "electron:build": "npm run build && electron-builder --win",
    "dist": "npm run build && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "electron-updater": "^6.7.3",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/extract-zip": "^2.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "wait-on": "^7.2.0"
  }
}